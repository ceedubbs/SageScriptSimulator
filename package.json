{
  "name": "aragon-callscript-simulator",
  "version": "0.0.0",
  "description": " Simulate the effect of a callscript script before you put it to a vote",
  "engines": {
    "node": ">= 18.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.39",
    "@typescript-eslint/parser": "~5.39",
    "axios": "^0.26.1",
    "dotenv": "^10.0.0",
    "eslint": "~8.24",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "ethers": "^5.6.1",
    "hardhat": "^2.11.2",
    "prettier": "~2.7",
    "rimraf": "~3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.1.0",
    "tsutils": "~3.21",
    "typescript": "~4.8"
  },
  "scripts": {
    "dryrun": "ts-node src/index.ts",
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "enquirer": "^2.3.6"
  }
}